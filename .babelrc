{
  "presets": ["@babel/preset-env"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@config": "./config",
          "@public": "./public",
          "@utils": "./src/utils",
          "@models": "./src/models",
          "@methods": "./src/methods",
          "@routes": "./src/routes"
        }
      }
    ]
  ]
}
